bool isPalindrome(char *str) {
    for (int i=0;i<strlen(str)/2;i++) {
        if (str[i]!=str[strlen(str)-1-i]) {//–æ–≤–¥–µ –µ —Å–æ –Ω–µ–≥–∞—Ü–∏—ò–∞ –±–∏–¥–µ—ò—ú–∏ –∑–∞ –¥–∞ –±–∏–¥–µ –ø–∞–ª–∏–Ω–¥—Ä–æ–º –Ω–µ —Å–º–µ–µ –Ω–∏—Ç—É –µ–¥–Ω–∞ –¥–∞ –µ —Ä–∞–∑–ª–∏—á–Ω–∞ ,–∞–∫–æ —Å—Ç–∞–≤–µ–≤–º–µ –æ–±—Ä–∞—Ç–Ω–∞ –ª–æ–≥–∏–∫–∞ —Ç.–µ –∞–∫–æ —Å–µ –∏—Å—Ç–∏ –¥–∞ –≤—Ä–∞—Ç–∏ true —Ç–æ–∞ —ú–µ –≤—Ä–∞—Ç–∏ true
//–æ–¥–º–∞ –±–µ–∑ –¥–∞ —ò–∞ –ø—Ä–æ–≤–µ—Ä–∏ —Ü–µ–ª–∞—Ç–∞ –Ω–∏–∑–∞,–∑–∞—Ç–æ–∞ return true –µ –Ω–∞–¥–≤–æ—Ä –æ–¥ for loop-–æ—Ç –∫–æ–≥–∞ —ú–µ —Å–µ –∑–∞–≤—Ä—à–∏ —Å–æ –ø—Ä–æ–≤–µ—Ä–∫–∞—Ç–∞ –∞–∫–æ –Ω–µ –µ –Ω–∞—ò–¥–µ–Ω–∞ —Ä–∞–∑–ª–∏—á–Ω–∞ —Ç–æ–≥–∞—à –µ –ø–∞–ª–∏–Ω–¥—Ä–æ–º
            return false;
        }
    }
    return true;
}





