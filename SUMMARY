12.–°–µ–∫–æ—ò–∞ –±—É–∫–≤–∞ —Å–µ –∑–∞–º–µ–Ω—É–≤–∞ —Å–æ –±—É–∫–≤–∞—Ç–∞ —à—Ç–æ –µ 3 –º–µ—Å—Ç–∞ –ø–æ–Ω–∞–ø—Ä–µ–¥ –≤–æ –∞–∑–±—É–∫–∞—Ç–∞ .–ó–∞ –±—É–∫–≤–∏—Ç–µ –Ω–∞ –∫—Ä–∞—ò–æ—Ç –æ–¥ –∞–∑–±—É–∫–∞—Ç–∞, –±—Ä–æ–µ—ö–µ—Ç–æ –ø—Ä–æ–¥–æ–ª–∂—É–≤–∞ —Ü–∏–∫–ª–∏—á–Ω–æ –æ–¥ –ø–æ—á–µ—Ç–æ–∫–æ—Ç –Ω–∞ –∞–∑–±—É–∫–∞—Ç–∞.

char shiftChar(char c) {
    c=tolower(c);
    if (isalpha(c)) {
        c=((c-'a'+3)%26)+'a';
    }
    return c;
}


c - 'a' ‚Üí —ò–∞ –Ω–∞–æ—ì–∞ –ø–æ–∑–∏—Ü–∏—ò–∞—Ç–∞ –Ω–∞ –±—É–∫–≤–∞—Ç–∞ –≤–æ –∞–∑–±—É–∫–∞—Ç–∞ (–ø—Ä–∏–º–µ—Ä 'a' = 0, 'b' = 1, ‚Ä¶ 'z' = 25).

+ 3 ‚Üí –ø–æ–º–µ—Å—Ç—É–≤–∞ 3 –º–µ—Å—Ç–∞ –Ω–∞–ø—Ä–µ–¥.

% 26 ‚Üí –∑–∞ –¥–∞ –æ—Å—Ç–∞–Ω–µ –≤–æ –≥—Ä–∞–Ω–∏—Ü–∏ –Ω–∞ –∞–∑–±—É–∫–∞—Ç–∞ (–∞–∫–æ —Å—Ç–∏–≥–Ω–µ–º–µ –ø–æ—Å–ª–µ 'z', –¥–∞ —Å–µ –≤—Ä–∞—Ç–∏ –Ω–∞ –ø–æ—á–µ—Ç–æ–∫).

+ 'a' ‚Üí —ò–∞ –≤—Ä–∞—ú–∞ –Ω–∞–∑–∞–¥ –≤–æ –±—É–∫–≤–∞.



13.–ö–û–õ–ö–£ –ü–ê–¢–ò –°–ï –ü–û–à–ê–í–£–í–ê –ù–ê–à–ú–ê–õ–¶–ï –ó–ù–ê–ß–ê–à–ù–ê–¢–ê –¶–ò–§–†–ê –í–û 1 –ë–†–û–à -–ü–û–°–õ–ï–î–ù–ê –¶–ò–§–†–ê –ï –ù–ê–à–ú–ê–õ–¶–ï –ó–ù–ê–ß–ê–à–ù–ê %10

int najmalceZnacajna(int n) {
    int temp=n;
    int last=temp%10;
    int counter=0;
while (n>0) {
    if (last==n%10)
    counter++;
    n=n/10;
}
return counter;


}



14./*–ù–∞–ø–∏—à–∏ –ø—Ä–æ–≥—Ä–∞–º–∞ –∫–æ—ò–∞ —ú–µ —á–∏—Ç–∞ –Ω–µ–ø–æ–∑–Ω–∞—Ç –±—Ä–æ—ò –æ–¥ —Ç—Ä–∏ —Ü–µ–ª–∏ –±—Ä–æ–µ–≤–∏, –∏ –∑–∞ —Å–µ–∫–æ—ò–∞ —Ç—Ä–æ—ò–∫–∞ (a, b, c) —ú–µ –≥–æ –∏—Å–ø–µ—á–∞—Ç–∏ –±—Ä–æ—ò–æ—Ç
 *(a –∏–ª–∏ b) —à—Ç–æ —ò–∞ —Å–æ–¥—Ä–∂–∏ —Ü–∏—Ñ—Ä–∞—Ç–∞ c –ø–æ–≤–µ—ú–µ –ø–∞—Ç–∏ –Ω–∞ –ø–∞—Ä–Ω–∏ –ø–æ–∑–∏—Ü–∏–∏ (–ø–æ–∑–∏—Ü–∏–∏—Ç–µ —Å–µ –±—Ä–æ—ò–∞—Ç –æ–¥ –¥–µ—Å–Ω–æ –∫–æ–Ω –ª–µ–≤–æ, –∞ –ø—Ä–≤–∞—Ç–∞ –ø–æ–∑–∏—Ü–∏—ò–∞ –µ 1).
 */
#include<iostream>
using namespace std;
int cifraCTimes(int n,int c) {
    int counter=0;
    int pos=1;
    while (n>0) {
        int cifra=n%10;
        if (cifra==c&&pos%2==0) {
            counter++;

        }
        pos++;
        n=n/10;
    }
    return counter;
}
int main() {
    int a,b,c;
    while (cin>>a>>b>>c) {
int countA=cifraCTimes(a,c);
       int countB= cifraCTimes(b,c);
        if (countA>=countB) {
            cout<<a<<endl;

        }
        else {
            cout<<b<<endl;
        }
    }
}


15./*
 *–ü—Ä–æ—á–∏—Ç–∞—ò –≥–∏ –¥–∏–º–µ–Ω–∑–∏–∏—Ç–µ –Ω–∞ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ ,–ø–∞ —Å–∞–º–∞—Ç–∞ –º–∞—Ç—Ä–∏—Ü–∞
 *–ü—Ä–æ—á–∏—Ç–∞—ò—Ç–µ –≥–∏ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∏—Ç–µ –Ω–∞ –µ–¥–µ–Ω –µ–ª–µ–º–µ–Ω—Ç –æ–¥ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞.
 *–ê–∫–æ –µ–ª–µ–º–µ–Ω—Ç–æ—Ç –Ω–∞ –ø–æ–∑–∏—Ü–∏—ò–∞—Ç–∞ –∑–∞ —á–∏—Ç–∞—ö–µ –µ 0, —Ç–æ–≥–∞—à —Ç—Ä–µ–±–∞ –¥–∞ —Å—Ç–∞–Ω–µ 1 –∏:
—Å–∏—Ç–µ –Ω—É–ª–∏ –Ω–∞–¥ —Ç–æ—ò –µ–ª–µ–º–µ–Ω—Ç –¥–æ –ø—Ä–≤–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —Å–æ –≤—Ä–µ–¥–Ω–æ—Å—Ç 1 –∏–ª–∏ –¥–æ –ø–æ—á–µ—Ç–æ–∫–æ—Ç –Ω–∞ –∫–æ–ª–æ–Ω–∞—Ç–∞,
—Å–∏—Ç–µ –Ω—É–ª–∏ –ø–æ–¥ —Ç–æ—ò –µ–ª–µ–º–µ–Ω—Ç –¥–æ –ø—Ä–≤–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —Å–æ –≤—Ä–µ–¥–Ω–æ—Å—Ç 1 –∏–ª–∏ –¥–æ –∫—Ä–∞—ò–æ—Ç –Ω–∞ –∫–æ–ª–æ–Ω–∞—Ç–∞,
—Å–∏—Ç–µ –Ω—É–ª–∏ –ª–µ–≤–æ –æ–¥ —Ç–æ—ò –µ–ª–µ–º–µ–Ω—Ç –¥–æ –ø—Ä–≤–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —Å–æ –≤—Ä–µ–¥–Ω–æ—Å—Ç 1 –∏–ª–∏ –¥–æ –ø–æ—á–µ—Ç–æ–∫–æ—Ç –Ω–∞ —Ä–µ–¥–æ—Ç –∏
—Å–∏—Ç–µ –Ω—É–ª–∏ –¥–µ—Å–Ω–æ –æ–¥ —Ç–æ—ò –µ–ª–µ–º–µ–Ω—Ç –¥–æ –ø—Ä–≤–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —Å–æ –≤—Ä–µ–¥–Ω–æ—Å—Ç 1 –∏–ª–∏ –¥–æ –∫—Ä–∞—ò–æ—Ç –Ω–∞ —Ä–µ–¥–æ—Ç.
–ê–∫–æ –µ–ª–µ–º–µ–Ω—Ç–æ—Ç –Ω–∞ –¥–∞–¥–µ–Ω–∞—Ç–∞ –ø–æ–∑–∏—Ü–∏—ò–∞ –µ 1, —Ç–æ–≥–∞—à –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ –æ—Å—Ç–∞–Ω—É–≤–∞ –Ω–µ–ø—Ä–æ–º–µ–Ω–µ—Ç–∞.
–ò—Å–ø–µ—á–∞—Ç–µ—Ç–µ —ò–∞ –ø—Ä–æ–º–µ–Ω–µ—Ç–∞—Ç–∞ –º–∞—Ç—Ä–∏—Ü–∞.
 */
#include <iostream>
using namespace std;

int main() {
 int m, n;
 cin >> m >> n;
 int A[100][100];

 // –í—á–∏—Ç—É–≤–∞—ö–µ –Ω–∞ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞
 for (int i = 0; i < m; i++)
  for (int j = 0; j < n; j++)
   cin >> A[i][j];

 int x, y;
 cin >> x >> y; // –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∏ –Ω–∞ –µ–ª–µ–º–µ–Ω—Ç–æ—Ç

 // –ü—Ä–æ–≤–µ—Ä–∫–∞ –¥–∞–ª–∏ –µ 0
 if (A[x][y] == 0) {
  A[x][y] = 1;

  // –ì–æ—Ä–µ
  for (int i = x-1; i >= 0; i--) {
   if (A[i][y] == 1) break;
   A[i][y] = 1;
  }

  // –î–æ–ª—É
  for (int i = x+1; i < m; i++) {
   if (A[i][y] == 1) break;
   A[i][y] = 1;
  }

  // –õ–µ–≤–æ
  for (int j = y-1; j >= 0; j--) {
   if (A[x][j] == 1) break;
   A[x][j] = 1;
  }

  // –î–µ—Å–Ω–æ
  for (int j = y+1; j < n; j++) {
   if (A[x][j] == 1) break;
   A[x][j] = 1;
  }
 }

 // –ü–µ—á–∞—Ç–µ—ö–µ –Ω–∞ —Ä–µ–∑—É–ª—Ç–∞—Ç–æ—Ç
 for (int i = 0; i < m; i++) {
  for (int j = 0; j < n; j++) {
   cout << A[i][j] << " ";
  }
  cout << endl;
 }

 return 0;
}


16./*
 *–ù–∞–ø–∏—à–µ—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–∞ –∫–∞–¥–µ —à—Ç–æ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ –ê –ø—Ä–æ—á–∏—Ç–∞–Ω–∞ –æ–¥ SI (–º–∞–∫—Å–∏–º–∞–ª–Ω–∞—Ç–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç –Ω–∞ –¥–∏–º–µ–Ω–∑–∏–∏—Ç–µ –Ω–∞ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ –µ 100)
 —Å–µ —Ç—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∏—Ä–∞ –≤–æ –Ω–æ–≤–∞ –º–∞—Ç—Ä–∏—Ü–∞ B. –°–µ–∫–æ—ò –µ–ª–µ–º–µ–Ω—Ç –æ–¥ –Ω–æ–≤–∞—Ç–∞ –º–∞—Ç—Ä–∏—Ü–∞ B –µ –∑–±–∏—Ä –æ–¥ –ø–æ–∑–∏—Ç–∏–≤–Ω–∏ —Å–æ—Å–µ–¥–∏ –Ω–∞ —Å–æ–æ–¥–≤–µ—Ç–Ω–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç
 –æ–¥ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ A. –ò—Å–ø–µ—á–∞—Ç–µ—Ç–µ —ò–∞ –Ω–æ–≤–∞—Ç–∞ –º–∞—Ç—Ä–∏—Ü–∞ B –Ω–∞ —Å—Ç–∞–Ω–¥–∞—Ä–¥–Ω–∏–æ—Ç –∏–∑–ª–µ–∑.
 */
#include<iostream>
using namespace std;
int main() {
    int n,m;
    int matrica[100][100];
    int b[100][100];
    cin>>n>>m;
    for (int i=0;i<n;i++) {
        for (int j=0;j<m;j++) {
            cin>>matrica[i][j];
        }
    }
for (int i=0;i<n;i++) {
    for (int j=0;j<m;j++) {
        int suma=0;
        //gore
        if (i-1>=0 && matrica[i-1][j]>0) {
            suma+=matrica[i-1][j];
        }
        //dole
        if (i+1<n &&matrica[i+1][j]>0) {
            suma+=matrica[i+1][j];
        }
        //levo
        if (j-1>=0 && matrica[i][j-1]>0) {
            suma+=matrica[i][j-1];
        }
        //desno
        if (j+1<m &&matrica[i][j+1]>0) {
            suma+=matrica[i][j+1];
        }
        b[i][j]=suma;
    }
}
    for (int i=0;i<n;i++) {
        for (int j=0;j<m;j++) {
            cout<<b[i][j];
        }
        cout<<endl;
    }



}


17.
/*
 *–ù–∞–ø–∏—à–µ—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–∞ –∫–æ—ò–∞ —ú–µ —á–∏—Ç–∞ –Ω–∏–∑–∞ (–Ω–µ –ø–æ–¥–æ–ª–≥–∞ –æ–¥ 100 –∑–Ω–∞—Ü–∏) —à—Ç–æ —Å–æ–¥—Ä–∂–∏ –±—É–∫–≤–∏ –∏ —Ü–∏—Ñ—Ä–∏.
 *–ü—Ä–æ–≥—Ä–∞–º–∞—Ç–∞ —Ç—Ä–µ–±–∞ –¥–∞ –≥–∏ –∏—Å–ø–µ—á–∞—Ç–∏ –Ω–∞ —Å—Ç–∞–Ω–¥–∞—Ä–¥–Ω–∏–æ—Ç –∏–∑–ª–µ–∑ –±—É–∫–≤–∏—Ç–µ —à—Ç–æ —Å–µ –Ω–∞–æ—ì–∞–∞—Ç –ø–æ–º–µ—ì—É –ø—Ä–≤–∞—Ç–∞ –∏ –≤—Ç–æ—Ä–∞—Ç–∞ —Ü–∏—Ñ—Ä–∞,
 *–æ–¥ –ª–µ–≤–æ –∫–æ–Ω –¥–µ—Å–Ω–æ, –≤–∫–ª—É—á—É–≤–∞—ò—ú–∏ –≥–∏ –∏ —Å–∞–º–∏—Ç–µ —Ü–∏—Ñ—Ä–∏ —à—Ç–æ —ò–∞ –≥—Ä–∞–Ω–∏—á–∞—Ç. –ê–∫–æ –Ω–∏–∑–∞—Ç–∞ –∏–º–∞ —Å–∞–º–æ –µ–¥–Ω–∞ —Ü–∏—Ñ—Ä–∞, –ø—Ä–æ–≥—Ä–∞–º–∞—Ç–∞ —Ç—Ä–µ–±–∞ –¥–∞
–≥–∏ –∏—Å–ø–µ—á–∞—Ç–∏ —Å–∏—Ç–µ –±—É–∫–≤–∏ –¥–æ –∫—Ä–∞—ò–æ—Ç –Ω–∞ –Ω–∏–∑–∞—Ç–∞. –ê–∫–æ –Ω–∏–∑–∞—Ç–∞ –Ω–µ–º–∞ —Ü–∏—Ñ—Ä–∏, –ø—Ä–æ–≥—Ä–∞–º–∞—Ç–∞ —Ç—Ä–µ–±–∞ –¥–∞ —ò–∞ –∏—Å–ø–µ—á–∞—Ç–∏ –ø–æ—Ä–∞–∫–∞—Ç–∞ ‚Äû–ù–µ–º–∞ —Ü–∏—Ñ—Ä–∏‚Äú
 */
#include<iostream>
#include<cstring>
using namespace std;
int main() {
 char str[100];
 int firstDigit=-1;
 int secondDigit=-1;
 cin.getline(str,100);
 //bukvi megju prva i vtora cifra
 for (int i=0;i<strlen(str);i++) {
  if (isdigit(str[i])) {
   if (firstDigit==-1){
    firstDigit=i;
   }
   else{
    secondDigit=i;
    break;//najdena e vtorata
   }
  }
 }

  if (secondDigit==-1) {//ako ima samo 1 cifra
   for (int i=firstDigit;i<strlen(str);i++) {
    cout<<str[i];
   }
  }
  else if (firstDigit==-1&&secondDigit==-1) {
   cout<<"Nema cifri";
  }
  else {
   for (int i=firstDigit;i<=secondDigit;i++) {
    cout<<str[i];
   }

  }
 }




18./*
*–ù–∞–ø–∏—à–µ—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–∞ —à—Ç–æ —ú–µ —ò–∞ —Ç—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∏—Ä–∞ –Ω–∏–∑–∞—Ç–∞ –Ω–∞ —Å–ª–µ–¥–Ω–∏–æ–≤ –Ω–∞—á–∏–Ω: –ø–æ—á–Ω—É–≤–∞—ò—ú–∏ –æ–¥ –ø–æ—á–µ—Ç–æ–∫–æ—Ç –Ω–∞ –Ω–∏–∑–∞—Ç–∞,
–∑–∞ —Å–µ–∫–æ—ò –µ–ª–µ–º–µ–Ω—Ç —à—Ç–æ –µ –µ–¥–Ω–∞–∫–æ–≤ –Ω–∞ —Å–ª–µ–¥–Ω–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç –≤–æ –Ω–∏–∑–∞—Ç–∞, —É–¥–≤–æ—ò—Ç–µ —ò–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç–∞ –Ω–∞ –ø—Ä–≤–∏–æ—Ç –∏ –ø—Ä–µ–±—Ä–∏—à–µ—Ç–µ —ò–∞
–≤—Ç–æ—Ä–∞—Ç–∞ —Å–æ 0. –ü–æ—Å–ª–µ —Ç–æ–∞, –ø—Ä–µ—É—Ä–µ–¥–µ—Ç–µ —ò–∞ –Ω–∏–∑–∞—Ç–∞ —Ç–∞–∫–∞ —à—Ç–æ —Å–∏—Ç–µ –Ω—É–ª–∏ —ú–µ –±–∏–¥–∞—Ç –ø–æ–º–µ—Å—Ç–µ–Ω–∏ –Ω–∞ –∫—Ä–∞—ò–æ—Ç –æ–¥ –Ω–∏–∑–∞—Ç–∞,
–∑–∞–¥—Ä–∂—É–≤–∞—ò—ú–∏ –≥–æ —Ä–µ–¥–æ—Å–ª–µ–¥–æ—Ç –Ω–∞ –¥—Ä—É–≥–∏—Ç–µ –µ–ª–µ–º–µ–Ω—Ç–∏. –ò—Å–ø–µ—á–∞—Ç–µ—Ç–µ –≥–∏ —Å–∏—Ç–µ –µ–ª–µ–º–µ–Ω—Ç–∏ –æ–¥ –Ω–∏–∑–∞—Ç–∞ –Ω–∞ SO –≤–æ –µ–¥–µ–Ω —Ä–µ–¥,
–æ–¥–¥–µ–ª–µ–Ω–∏ —Å–æ –ø—Ä–∞–∑–Ω–æ –º–µ—Å—Ç–æ.
 */
#include<iostream>
using namespace std;
void transform(int a[],int n) {
    for (int i=0;i<n-1;i++) {
        if (a[i]==a[i+1]) {
            a[i]=a[i]*2;
            a[i+1]=0;
        }
    }
}
void pomestiNuliNaKraj(int a[],int n) {
    int pos=0;
    for (int i=0;i<n;i++) {
        if (a[i]!=0) {
            a[pos++]=a[i];
        }

    }
    while (pos<n) {
        a[pos++]=0;
    }

}
int main() {
    int n;
    int niza[100];
    cin>>n;
    for (int i=0;i<n;i++) {
        cin>>niza[i];
    }
    transform(niza,n);
    pomestiNuliNaKraj(niza,n);
    for (int i=0;i<n;i++) {
        cout<<niza[i]<<" "<<endl;
    }

}



19./*
 *–¢—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∏—Ä–∞—ò—Ç–µ —ò–∞ –º–∞—Ç—Ä–∏—Ü–∞—Ç–∞ –Ω–∞ –Ω–∞—á–∏–Ω —à—Ç–æ —Å–∏—Ç–µ –µ–ª–µ–º–µ–Ω—Ç–∏ –ù–ê–î –≥–ª–∞–≤–Ω–∞—Ç–∞ –¥–∏—ò–∞–≥–æ–Ω–∞–ª–∞ —ú–µ –±–∏–¥–∞—Ç –ø–æ—Å—Ç–∞–≤–µ–Ω–∏ –Ω–∞
 *–≤—Ä–µ–¥–Ω–æ—Å—Ç–∞ –Ω–∞ –º–∞–∫—Å–∏–º–∞–ª–Ω–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —à—Ç–æ —Å–µ –Ω–∞–æ—ì–∞ –Ω–∞–¥ –≥–ª–∞–≤–Ω–∞—Ç–∞ –¥–∏—ò–∞–≥–æ–Ω–∞–ª–∞, –¥–æ–¥–µ–∫–∞ —Å–∏—Ç–µ –µ–ª–µ–º–µ–Ω—Ç–∏ –ü–û–î –≥–ª–∞–≤–Ω–∞—Ç–∞
 *–¥–∏—ò–∞–≥–æ–Ω–∞–ª–∞ —ú–µ –±–∏–¥–∞—Ç –ø–æ—Å—Ç–∞–≤–µ–Ω–∏ –Ω–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç–∞ –Ω–∞ –º–∏–Ω–∏–º–∞–ª–Ω–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —à—Ç–æ —Å–µ –Ω–∞–æ—ì–∞ –ø–æ–¥ –≥–ª–∞–≤–Ω–∞—Ç–∞ –¥–∏—ò–∞–≥–æ–Ω–∞–ª–∞.
 *–ò—Å–ø–µ—á–∞—Ç–µ—Ç–µ —ò–∞ —Ç—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∏—Ä–∞–Ω–∞—Ç–∞ –º–∞—Ç—Ä–∏—Ü–∞.
 */
#include<iostream>
using namespace std;
int main() {
    int n;
    int matrica[100][100];
    cin>>n;
    for (int i=0;i<n;i++) {
        for (int j=0;j<n;j++) {
            cin>>matrica[i][j];
        }
    }
    //nad glavna dijagonala
    int sum=0;
    int max=matrica[0][1];
    int min=matrica[1][0];
    for (int i=0;i<n;i++) {
        for (int j=0;j<n;j++) {
            if (j>i) {
                if (matrica[i][j]>max) {

                    max=matrica[i][j];
                }
            }
        }
    }
//pod glavna
    for (int i=0;i<n;i++) {
        for (int j=0;j<n;j++) {
            if (j<i) {
                if (matrica[i][j]<min) {

                    min=matrica[i][j];
                }
            }
        }
    }
    for (int i=0;i<n;i++) {
        for (int j=0;j<n;j++) {
            if (j>i) {
                matrica[i][j]=max;
            }
        }
    }
    for (int i=0;i<n;i++) {
        for (int j=0;j<n;j++) {
            if (j<i) {
                matrica[i][j]=min;
            }
        }
    }
    for (int i=0;i<n;i++) {

        for (int j=0;j<n;j++) {
            cout<<matrica[i][j]<<" ";
        }
        cout<<endl;
    }

}



20.–§–ê–ö–¢–û–†–ò–ï–õ
–†–µ–∫—É—Ä–∑–∏–≤–Ω–∞ —Ñ—É–Ω–∫—Ü–∏—ò–∞ –∑–∞ —Ñ–∞–∫—Ç–æ—Ä–∏–µ–ª
int factorial(int n) {
    if (n <= 1) return 1;
return n * factorial(n - 1);
 }

21.–û–ë–†–ê–¢–ï–ù –ë–†–û–à

int obraten(int n) {
    int reverse=0;
    int temp=n;
    while (temp>0) {
        int cifra=temp%10;
        reverse=reverse*10+cifra;
        temp/=10;
    }
    return reverse;


22.–ó–ë–ò–† –ù–ê –î–ï–õ–ò–¢–ï–õ–ò,–ù–ï –°–ú–ï–ï–® –û–î –ù–£–õ–ê –î–ê –ü–û–ß–ù–ï–®


int zbirDeliteli(int n) {
    int sum=0;
    for (int i=1;i<=n/2;i++) {//–¥–∞ –Ω–µ —Å–µ –∑–µ–º–∞ –ø—Ä–µ–¥–≤–∏–¥ —Å–∞–º–∏–æ—Ç –±—Ä–æ—ò,–æ–¥–∏ –¥–æ n/2
        //–∑–±–∏—Ä –Ω–∞ –¥–µ–ª–∏—Ç–µ–ª–∏ –Ω–µ —Å–º–µ–µ—à –æ–¥ 0 –¥–∞ –ø–æ—á–Ω–µ—à
        if (n%i==0) {
            sum+=i;
        }
    }
    return sum;
}


23.*–û–¥ —Å—Ç–∞–Ω–¥–∞—Ä–¥–µ–Ω –≤–ª–µ–∑ —Å–µ —á–∏—Ç–∞–∞—Ç –∑–Ω–∞—Ü–∏ —Å–µ –¥–æ–¥–µ–∫–∞ –Ω–µ —Å–µ –ø—Ä–æ—á–∏—Ç–∞ –∏–∑–≤–∏—á–Ω–∏–∫. –í–æ –≤–∞–∫–∞ –≤–Ω–µ—Å–µ–Ω–∏–æ—Ç —Ç–µ–∫—Å—Ç —Å–µ —Å–∫—Ä–∏–µ–Ω–∏ —Ü–µ–ª–∏ –±—Ä–æ–µ–≤–∏
(–ø–æ–º–∞–ª–∏ –æ–¥ 100). –î–∞ —Å–µ –Ω–∞–ø–∏—à–µ –ø—Ä–æ–≥—Ä–∞–º–∞ —à—Ç–æ —ú–µ –≥–∏ –ø—Ä–æ—á–∏—Ç–∞ —Å–∏—Ç–µ –∑–Ω–∞—Ü–∏ –∏ –Ω–∞ –∏–∑–ª–µ–∑ —ú–µ –≥–æ –∏—Å–ø–µ—á–∞—Ç–∏ –∑–±–∏—Ä–æ—Ç –Ω–∞ —Å–∏—Ç–µ –±—Ä–æ–µ–≤–∏
—Å–∫—Ä–∏–µ–Ω–∏ –≤–æ —Ç–µ–∫—Å—Ç–æ—Ç.
–ù–∞–ø–æ–º–µ–Ω–∞: cin –≤–æ C++ –≥–∏ –∏–≥–Ω–æ—Ä–∏—Ä–∞ –ø—Ä–∞–∑–Ω–∏—Ç–µ –º–µ—Å—Ç–∞ (space). –ó–∞ –¥–∞ –Ω–µ –≥–∏ –∏–≥–Ω–æ—Ä–∏—Ä–∞ –ø–æ—Ç—Ä–µ–±–Ω–æ –µ –¥–∞ —Å–µ –¥–æ–¥–∞–¥–µ noskipws
–ø—Ä–µ–¥ —á–∏—Ç–∞—ö–µ—Ç–æ –Ω–∞ –∑–Ω–∞–∫–æ—Ç (–ø—Ä. cin >> noskipws >> znak;)
 */
#include <iostream>
#include <cctype>
using namespace std;

int main() {
    char znak;
    int sum = 0;
    int num = 0;

    while (cin >> noskipws >> znak) {

        if (znak == '!') break;

        if (isdigit(znak)) {
            num = num * 10 + (znak - '0');//–±—Ä–æ—ò–æ—Ç —à—Ç–æ –≥–æ –≥—Ä–∞–¥–∏–º–µ
        } else {
            sum += num;
            num = 0;
        }
    }
    /*–ê–∫–æ —Ç–µ–∫—Å—Ç–æ—Ç –∑–∞–≤—Ä—à—É–≤–∞ —Å–æ –±—Ä–æ—ò, –Ω–∞ –ø—Ä–∏–º–µ—Ä:
  abc12!
  –¶–∏–∫–ª—É—Å–æ—Ç —ú–µ —á–∏—Ç–∞ '1' –∏ '2', num = 12.
  –ù–æ –∫–æ–≥–∞ —ú–µ —Å–µ –ø—Ä–æ—á–∏—Ç–∞ '!', –≤–ª–µ–≥—É–≤–∞—à –≤–æ if (znak == '!') break; –∏ –Ω–∏–∫–æ–≥–∞—à –Ω–µ –≥–æ –¥–æ–¥–∞–≤–∞—à –ø–æ—Å–ª–µ–¥–Ω–∏–æ—Ç –±—Ä–æ—ò (num) –≤–æ sum.
  üí° –ó–Ω–∞—á–∏, –ø–æ—Å–ª–µ–¥–Ω–∏–æ—Ç –±—Ä–æ—ò —Å–µ –≥—É–±–∏ –∞–∫–æ —Ç–µ–∫—Å—Ç–æ—Ç –∑–∞–≤—Ä—à—É–≤–∞ –¥–∏—Ä–µ–∫—Ç–Ω–æ —Å–æ !.,–±–∏–¥–µ—ò—ú–∏ –≤–æ if –≥–æ –≥—Ä–∞–¥–∏—à,–∞ –≥–æ –¥–æ–¥–∞–≤–∞—à
  //–≤–æ else ,—Ç.–µ –∫–æ–≥–∞ –Ω–∞–∏–¥—É–≤–∞—à –Ω–∞ –Ω–µ—à—Ç–æ —à—Ç–æ –Ω–µ –µ –±—Ä–æ—ò,–æ–¥ —Ç–∞–∞ –ø—Ä–∏—á–∏–Ω–∞ –±–∏–¥–µ—ò—ú–∏  if (znak == '!') break;
  –æ–¥–º–∞ –≥–æ –ø—Ä–µ–∫–∏–Ω—É–≤–∞ while —Ü–∏–∫–ª—É—Å–æ—Ç –∏ –Ω–µ —Å–µ –∏–∑–≤—Ä—à—É–≤–∞ –Ω–∏—à—Ç–æ –ø–æ–¥–æ–ª—É –∑–∞—Ç–æ–∞ –Ω–∞–¥–≤–æ—Ä –æ–¥ while –ø—Ä–∞–≤–∏–º–µ sum+=num
   */
    sum += num;

    cout << sum << endl;
}



24./*
–¢–û–¢–ê–õ–ù–û –†–ê–ó–õ–ò–ß–ï–ù –ë–†–û–à –ü–û–ú–ê–õ –û–î N
–ï–¥–µ–Ω –±—Ä–æ—ò –µ —Ç–æ—Ç–∞–ª–Ω–æ —Ä–∞–∑–ª–∏—á–µ–Ω –æ–¥ –¥—Ä—É–≥ –∞–∫–æ –∏ —Å–∞–º–æ –∞–∫–æ –≤–æ –Ω–µ–≥–æ –Ω–µ —Å–µ –ø–æ—ò–∞–≤—É–≤–∞ –Ω–∏—Ç—É –µ–¥–Ω–∞ –æ–¥ —Ü–∏—Ñ—Ä–∏—Ç–µ –Ω–∞ –¥—Ä—É–≥–∏–æ—Ç –±—Ä–æ—ò.

#include<iostream>
using namespace std;
int totalnoRazlicen(int n,int x) {
    //totalno razlicen od x

    while (n>0) {
        int temp2=x;
        int cifraN=temp1%10;
        while (temp2>0) {
            int cifraX=temp2%10;

            if (cifraN==cifraX) {
                return 0;

            }
            temp2=temp2/10;
        }
        n=n/10;
    }
    return 1;
}
int main() {
    int n,x;
    cin>>n>>x;
    for (int i=n-1;i>=0;i--) {
        if (totalnoRazlicen(i,x)) {
            cout << i << endl;
            break;
        }
    }
}


25.–°–¢–ê–¢–ò–°–¢–ò–ö–ê –ù–ê –ë–†–û–ï–í–ò
*–û–¥ —Ç–∞—Å—Ç–∞—Ç—É—Ä–∞ —Å–µ —á–∏—Ç–∞–∞—Ç –Ω–µ–ø–æ–∑–Ω–∞—Ç –±—Ä–æ—ò –Ω–∞ –ø–æ–∑–∏—Ç–∏–≤–Ω–∏ —Ü–µ–ª–∏ –±—Ä–æ–µ–≤–∏ (—Å–æ –º–∞–∫—Å–∏–º—É–º 5 —Ü–∏—Ñ—Ä–∏) —Å–µ –¥–æ–¥–µ–∫–∞ –Ω–µ —Å–µ –≤–Ω–µ—Å–µ –Ω–µ—à—Ç–æ —Ä–∞–∑–ª–∏—á–Ω–æ
*–æ–¥ –±—Ä–æ—ò. –ó–∞ —Å–µ–∫–æ—ò –±—Ä–æ—ò —Ç—Ä–µ–±–∞ –¥–∞ —Å–µ –Ω–∞—ò–¥–µ –ø–æ–∑–∏—Ü–∏—ò–∞—Ç–∞ (–æ–¥ –¥–µ—Å–Ω–æ –Ω–∞ –ª–µ–≤–æ) –Ω–∞ –ø—Ä–≤–æ—Ç–æ –ø–æ—ò–∞–≤—É–≤–∞—ö–µ –Ω–∞ –Ω–∞—ò–≥–æ–ª–µ–º–∞—Ç–∞ —Ü–∏—Ñ—Ä–∞ –≤–æ
*—Å–æ—Å—Ç–∞–≤–æ—Ç –Ω–∞ –±—Ä–æ—ò–æ—Ç (—Ü–∏—Ñ—Ä–∞—Ç–∞ –Ω–∞ –µ–¥–∏–Ω–∏—Ü–∏ —Å–µ –Ω–∞–æ—ì–∞ –Ω–∞ –ø–æ–∑–∏—Ü–∏—ò–∞ 0). –ü–æ—Ç–æ–∞, –¥–∞ —Å–µ –∏—Å–ø–µ—á–∞—Ç–∏ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –∑–∞
*–ø–æ–∑–∏—Ü–∏–∏—Ç–µ –Ω–∞ –Ω–∞—ò–¥–µ–Ω–∏—Ç–µ —Ü–∏—Ñ—Ä–∏ —Å–æ –º–∞–∫—Å–∏–º–∞–ª–Ω–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç –≤–æ —Å–ª–µ–¥–Ω–∏–æ—Ç —Ñ–æ—Ä–º–∞—Ç:
[–ø–æ–∑–∏—Ü–∏—ò–∞]: [–≤–∫—É–ø–Ω–æ –±—Ä–æ–µ–≤–∏ —á–∏—ò–∞ –º–∞–∫—Å–∏–º–∞–ª–Ω–∞ —Ü–∏—Ñ—Ä–∞ —Å–µ –Ω–∞–æ—ì–∞ –Ω–∞ —Ç–∞–∞ –ø–æ–∑–∏—Ü–∏—ò–∞]
0: 2

1: 3

2: 1

3: 1

4: 1

–∑–∞ –±—Ä–æ–µ–≤–∏—Ç–µ 97654 48654 12345 12343 1263 12443 12643 12777
 */
#include <iostream>
using namespace std;

int main() {
    int stats[5] = {0}; // —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –∑–∞ –ø–æ–∑–∏—Ü–∏–∏—Ç–µ 0 –¥–æ 4
    int number;

    while (cin >> number) {
        if (number <= 0 || number > 99999) continue; // –∏–≥–Ω–æ—Ä–∏—Ä–∞ –Ω–µ–≤–∞–ª–∏–¥–Ω–∏ –±—Ä–æ–µ–≤–∏

        int temp = number;
        int maxDigit = -1;
        int position = 0;
        int maxPos = -1;

        while (temp > 0) {
            int digit = temp % 10;
            if (digit > maxDigit) {
                maxDigit = digit;
                maxPos = position;
            }
            temp /= 10;
            position++;
        }


            stats[maxPos]++;
        }


    // –ü–µ—á–∞—Ç–∏ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞
    for (int i = 0; i < 5; i++) {
        cout << i << ": " << stats[i] << endl;
    }

    return 0;
}


26.–ù–∞—ò–º–∞–ª–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç –≤–æ –Ω–∏–∑–∞—Ç–∞ –∫–æ—ò —Å–µ –ø–æ—ò–∞–≤—É–≤–∞ –ø–∞—Ä–µ–Ω –±—Ä–æ—ò –ø–∞—Ç–∏ –≤–æ –Ω–∏–∑–∞—Ç–∞.


int minEl = -999999;
    bool found = false;

    for (int i = 0; i < n; i++) {
        int counter = 0;
        for (int j = 0; j < n; j++) {
            if (a[i] == a[j]) {
                counter++;
            }
        }

        if (counter % 2 == 0 && a[i] < minEl) {
            minEl = a[i];
            found = true;
        }
    }

27.–ê–ö–û –ù–ï–®–¢–û –ï –ù–ê–à–î–ï–ù–û –ò –ù–ï –°–ê–ö–ê–ú–ï –î–ê –ü–†–û–î–û–õ–ñ–ò–ú–ï –ü–û–ù–ê–¢–ê–ö–ê –ö–û–†–ò–°–¢–ò–ú–ï BREAK


